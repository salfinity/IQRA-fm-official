{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "lib/utils.js",
    "actions/sendEmail.js",
    "src/components/email/contact-form-email.js",
    "src/components/Contact/submit-btn.js",
    "src/app/adverts/salsabil/page.js",
    "src/app/adverts/qyt/page.js",
    "src/app/adverts/pembe/page.js",
    "src/app/adverts/kips/page.js",
    "src/components/Adverts/page.j",
    "src/components/Adverts/AdvertItem.js",
    "actions/get-sizes.js",
    "actions/get-products.js",
    "actions/get-product.js",
    "actions/get-phones.js",
    "actions/get-colors.js",
    "actions/get-category.js",
    "actions/get-categories.js",
    "actions/get-billboard.js",
    "src/app/profiles/schedule/ScheduleItem.js",
    "src/app/profiles/schedule/page.js",
    "src/app/profiles/schedule/Navigation.js",
    "src/app/profiles/schedule/DropdownMenu.js",
    "src/components/TypingAnimation.js",
    "pages/api/chat.js",
    "src/components/Quotes/AiChatModal.js",
    "src/app/adverts/unity/page.js"
  ],
  "exclude": ["node_modules"]
}
